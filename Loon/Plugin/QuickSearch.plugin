[Rewrite]

# Safari内输入快捷键+空格+搜索关键词快速指定搜索引擎搜索
# 先进入设置更改Safari默认搜索为DuckDuckGo

# bd = 百度搜索
# ydimg = yandex图片搜索
# bdimg = 百度图片搜索
# ghimg = 谷歌图片搜索
# by = Bing搜索
# wk = 维基搜索
# qm = 七麦搜索
# gh = GitHub搜索
# yh = 油猴搜索
# ggc = Google译至中
# gge = Google译至英
# ggj = Google译至日
# ytb = YouTube搜索
# bli = 哔哩哔哩搜索
# af = AcFun搜索
# ph = PornHub搜索
# db = 豆瓣搜索
# zh = 知乎搜索
# wb = 微博搜索
# tt = Twitter
# jd = 京东搜索
# tb = 淘宝搜索
# ddg = DuckDuckGo搜索
# icn = App Store切换至中国区 感谢@mieqq
# ius = App Store切换至美国区 感谢@mieqq
# ihk = App Store切换至香港区 感谢@mieqq
# itw = App Store切换至台湾区 感谢@mieqq
# ijp = App Store切换至日本区 感谢@mieqq
# ikr = App Store切换至韩国区 感谢@mieqq
# itr = App Store切换至土耳其区 感谢@mieqq
# 无快捷键则默认使用Google搜索


^https:\/\/duckduckgo\.com\/\?q=bd\+([^&]+).+ 302 https://www.baidu.com/s?wd=$1
^https:\/\/duckduckgo\.com\/\?q=ydimg\+([^&]+).+ 302 https://yandex.com/images/touch/search?lr=10636&text=$1
